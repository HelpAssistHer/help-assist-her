{
	"name": "help-assist-her",
	"version": "0.0.1",
	"description": "Women's Healthcare resources in the palm of your hand.",
	"main": "server.js",
	"private": true,
	"engines": {
		"node": "10.16.3"
	},
	"scripts": {
		"build:html": "babel-node client/tools/build-html.js",
		"clear-iv": "NODE_ENV=localhost node ./server/scripts/clear-in-verification-field.js",
		"geocode": "NODE_ENV=localhost node ./server/data-import/geocode.js",
		"lint": "./node_modules/.bin/eslint .",
		"lint-path": "./node_modules/.bin/eslint",
		"n": "nvm use default",
		"overwrite-fqhc-data": "NODE_ENV=localhost node ./server/data-import/overwrite-fqhc-data.js",
		"overwrite-pc-data": "NODE_ENV=localhost node ./server/data-import/overwrite-pc-data.js",
		"precommit": "pretty-quick --staged",
		"prepare": "webpack",
		"start": "NODE_ENV=localhost nodemon ./server/server.js",
		"startdb": "mongod --dbpath ~/.hah/data",
		"test": "NODE_ENV=test mocha 'server/test/test.js' --timeout 3000",
		"test-data": "NODE_ENV=localhost node ./server/data-import/test-data.js",
		"watch": "yarn && webpack --watch"
	},
